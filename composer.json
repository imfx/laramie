{
    "name": "pandorga/laramie",
    "description": "Laramie is a nice dashboard based on Laravel 5.8+ Framework",
    "keywords": [ "laramie", "admin", "laravel", "dashboard", "php"],
    "license": "MIT",
    "authors": [
        {
            "name": "Felix Ayala",
            "email": "felix1262@gmail.com",
            "homepage": "http://felixaya.la",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/imfx/laramie",
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "~5.1",
        "spatie/laravel-html": "^2.19",
        "spatie/laravel-activitylog": "^3.1",
        "spatie/laravel-permission": "^2.9",
        "laravelcollective/html":"^5.8.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Pandorga\\Laramie\\": "src"
        },
        "files": [
            "src/Helpers/laramie_helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Pandorga\\Laramie\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pandorga\\Laramie\\LaramieServiceProvider"
            ],
            "aliases": {
                "Laramie": "Pandorga\\Laramie\\Facades\\Laramie"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "0.3"

}
